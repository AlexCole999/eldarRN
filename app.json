{
  "expo": {
    "name": "ArendaGo",
    "slug": "AwesomeProject",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ArendaGoIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/ArendaGoIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ArendaGoIcon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.leonid25071993.AwesomeProject",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "newArchEnabled": true
    },
    "web": {
      "favicon": "./assets/ArendaGoIcon.png"
    },
    "extra": {
      "eas": {
        "projectId": "60e8073e-a671-4835-8785-2c577a25302e"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ]
  }
}